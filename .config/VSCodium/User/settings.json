{
  "todo-tree.tree.showScanModeButton": false,
  "workbench.iconTheme": "material-icon-theme",
  "go.useLanguageServer": true,
  "editor.fontFamily": "JetBrainsMono Nerd Font, FiraCode Nerd Font, Fira Code",
  "editor.fontLigatures": true,
  "editor.suggestSelection": "first",
  "workbench.startupEditor": "newUntitledFile",
  "eslint.packageManager": "pnpm",
  "eslint.format.enable": true,
  "eslint.alwaysShowStatus": true,
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "typescrypt",
    "svelte"
  ],
  "eslint.lintTask.enable": true,
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "html",
    "typescript",
    "svelte"
  ],
  "python.languageServer": "Microsoft",
  "editor.fontWeight": 550,
  "editor.fontSize": 16,
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "omnisharp.enableImportCompletion": true,
  "omnisharp.enableRoslynAnalyzers": true,
  "csharp.referencesCodeLens.enabled": false,
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "cSpell.userWords": [
    "cmake",
    "fyne",
    "gorm",
    "lzma",
    "minmax",
    "postgres",
    "prefetch",
    "Scriptable",
    "submodules",
    "vite",
    "windicss",
    "youtube"
  ],
  "sync.gist": "afff5cddf59660d6c6f909bf140cc05d",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.maxTokenizationLineLength": 200000,
  "cmake.configureOnOpen": true,
  "clangd.onConfigChanged": "restart",
  "C_Cpp.intelliSenseEngine": "Disabled",
  "go.toolsManagement.autoUpdate": true,
  "go.lintTool": "golangci-lint",
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "editor.showFoldingControls": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "emmet.includeLanguages": {
    "postcss": "css"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "cSpell.enableFiletypes": [
    "svelte"
  ],
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "clangd.path": "/home/billy/.config/VSCodium/User/globalStorage/llvm-vs-code-extensions.vscode-clangd/install/12.0.0/clangd_12.0.0/bin/clangd",
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "workbench.colorTheme": "Community Material Theme Palenight",
  "[c]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "explorer.confirmDragAndDrop": false,
  "C_Cpp.default.cppStandard": "c++17",
  "C_Cpp.default.cStandard": "c99",
  "omnisharp.useGlobalMono": "always",
  "cmake.generator": "Ninja",
  "cmake.configureArgs": [
    "-Wno-dev"
  ],
  "cSpell.ignoreWords": [],
  "editor.bracketPairColorization.enabled": true,
  "svelte.enable-ts-plugin": true,
  "tabnine.experimentalAutoImports": true,
  "vsicons.dontShowNewVersionMessage": true,
  "window.zoomLevel": 2,
  "javascript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.importModuleSpecifier": "relative",
  "css.validate": false,
}